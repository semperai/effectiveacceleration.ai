{
  "name": "@effectiveacceleration/contracts",
  "version": "1.1.1",
  "description": "Effective Acceleration decentralized marketplace",
  "author": "Kasumi Null <kasumi.null@yandex.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "clean:contracts": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "build": "yarn prettier '**/*.sol' --write && yarn clean:contracts && yarn compile",
    "test": "npx hardhat test",
    "size": "npx hardhat size-contracts",
    "prettier:check": "prettier --check .",
    "prettier:lint": "prettier --write .",
    "coverage": "npx hardhat coverage --sources MarketplaceV3.sol --sources MarketplaceDataV1.sol",
    "test:coverage": "npx hardhat coverage --sources MarketplaceV3.sol --sources MarketplaceDataV1.sol",
    "node": "npx hardhat node",
    "deploy_local": "npx hardhat --network localhost run scripts/000-deploy-marketplace.ts",
    "seed_local": "npx hardhat --network localhost marketplace:seed",
    "deploy_seed": "sleep 10 && npx run-s deploy_local seed_local",
    "local": "npx run-p node deploy_seed",
    "wagmi": "npx hardhat extract-abi --ts",
    "tsc": "npx tsc"
  },
  "files": [
    "README.md",
    "package.json",
    "dist",
    "wagmi",
    "scripts/config.json"
  ],
  "devDependencies": {
    "@arbitrum/nitro-contracts": "^1.2.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.4",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.5.0",
    "@scure/base": "^1.2.1",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/chai-subset": "^1.3.5",
    "@types/mocha": ">=9.1.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "hardhat": "^2.22.4",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-ts-plugin-abi-extractor": "^0.0.6",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-solidity": "^1.3.1",
    "solidity-coverage": "^0.8.12",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": "^5.3.3",
    "yesno": "^0.4.0"
  },
  "dependencies": {
    "@noble/ciphers": "^0.5.3",
    "@sablier/lockup": "^2.0.1",
    "viem": "^2.21.51"
  }
}
